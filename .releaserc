{
  "branches": [
    "master",
    {
      "name": "development",
      "prerelease": true,
      "channel": "prerelease"
    }
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": [
          {
            "type": "feat",
            "release": "minor"
          },
          {
            "type": "fix",
            "release": "patch"
          },
          {
            "type": "perf",
            "release": "patch"
          },
          {
            "type": "style",
            "release": "patch"
          },
          {
            "type": "test",
            "release": "patch"
          },
          {
            "type": "chore",
            "release": "patch"
          },
          {
            "type": "ci",
            "release": "patch"
          },
          {
            "type": "build",
            "release": "patch"
          },
          {
            "type": "revert",
            "release": "patch"
          },
          {
            "type": "docs",
            "release": false
          },
          {
            "type": "refactor",
            "release": "patch"
          },
          {
            "scope": "no-release",
            "release": false
          }
        ]
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "angular",
        "parserOpts": {
          "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES",
            "BREAKING"
          ]
        },
        "presetConfig": {
          "types": [
            {"type": "feat", "section": "\uD83D\uDEE0 Features"},
            {"type": "feature", "section": "\uD83D\uDEE0 Features"},
            {"type": "fix", "section": "üêõ Bug Fixes"},
            {"type": "perf", "section": "üî• Performance Improvements"},
            {"type": "revert", "section": "‚è© Reverts"},
            {"type": "docs", "section": "üìù Dokumentation"},
            {"type": "style", "section": "üé® Styles"},
            {"type": "refactor", "section": "‚Äçüíª Code Refactoring"},
            {"type": "test", "section": "‚úÖ Tests"},
            {"type": "build", "section": "ü§ñ Build System"},
            {"type": "ci", "section": "üîÅ Continuous Integration"},
            {"type": "chore", "section": "üßπ Chores"}
          ]
        }
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": "pack/*.tgz"
      }
    ]
  ]
}